{
  "name": "es51922",
  "version": "0.0.0",
  "private": true,
  "description": "Streaming Cyrustek ES51922 DMM data protocol implementation over serial",
  "main": "index.js",
  "module": "src/index.js",
  "scripts": {
    "posttest": "markdownlint --ignore node_modules . && eslint .",
    "test": "nyc mocha \"test/**/*.spec.js\"",
    "release": "standard-version",
    "compile-posix": "kaitai-struct-compiler src/es51922.ksy.yaml -t javascript && mv Es51922.js vendor/es51922.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boneskull/es51922"
  },
  "keywords": [
    "ut61e",
    "multimeter",
    "es51922",
    "cyrustek",
    "rxjs",
    "stream",
    "observable",
    "serial",
    "serialport",
    "data-logger",
    "signal-analyzer",
    "voltage",
    "current",
    "resistance",
    "ac",
    "dc",
    "kaitai"
  ],
  "author": "Christopher Hiller <boneskull@boneskull.com> (https://boneskull.com/)",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@serialport/parser-byte-length": "^2.0.2",
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "kaitai-struct": "^0.8.0-SNAPSHOT.11",
    "rxjs": "^6.5.2",
    "rxjs-stream": "^3.0.2",
    "serialport": "^7.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "js-yaml": "^3.13.1",
    "lint-staged": "^8.2.0",
    "markdownlint-cli": "^0.14.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2",
    "standard-version": "^6.0.1",
    "unexpected": "^11.6.0",
    "unexpected-sinon": "^10.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "endOfLine": "auto"
  }
}
