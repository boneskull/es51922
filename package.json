{
  "name": "es51922",
  "version": "0.1.0",
  "description": "Streaming Cyrustek ES51922 DMM data protocol implementation over serial",
  "keywords": [
    "ac",
    "current",
    "cyrustek",
    "data-logger",
    "dc",
    "es51922",
    "kaitai",
    "multimeter",
    "observable",
    "resistance",
    "rxjs",
    "serial",
    "serialport",
    "signal-analyzer",
    "stream",
    "ut61e",
    "voltage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boneskull/es51922"
  },
  "license": "Apache-2.0",
  "author": "Christopher Hiller <boneskull@boneskull.com> (https://boneskull.com/)",
  "files": [
    "dist",
    "src",
    "vendor"
  ],
  "main": "dist/es51922.js",
  "module": "dist/es51922.mjs",
  "scripts": {
    "compile-posix": "kaitai-struct-compiler ./src/es51922.ksy.yaml -t javascript && mv Es51922.js ./vendor/es51922.js",
    "release": "standard-version",
    "build": "rollup -c",
    "test": "nyc mocha \"test/**/*.spec.js\"",
    "posttest": "markdownlint --ignore node_modules . && eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "endOfLine": "auto",
    "singleQuote": true
  },
  "dependencies": {
    "@serialport/parser-byte-length": "^2.0.2",
    "dotenv": "^8.0.0",
    "kaitai-struct": "^0.8.0-SNAPSHOT.11",
    "rxjs": "^6.5.2",
    "rxjs-stream": "^3.0.2",
    "serialport": "^7.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "esm": "^3.2.25",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.0",
    "markdownlint-cli": "^0.14.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.0",
    "rollup": "^1.16.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-subpath-externals": "^3.0.0",
    "sinon": "^7.3.2",
    "standard-version": "^6.0.1",
    "unexpected": "^11.6.0",
    "unexpected-sinon": "^10.11.2"
  },
  "engines": {
    "node": ">=8"
  },
  "nyc": {
    "all": true,
    "include": "src"
  }
}
